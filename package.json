{
  "name": "dynamic-config-store",
  "version": "1.0.5",
  "description": "Simple configuration utility for deployments and libraries",
  "keywords": [
    "config",
    "node",
    "package"
  ],
  "homepage": "https://github.com/lostpebble/dynamic-config-store#readme",
  "bugs": {
    "url": "https://github.com/lostpebble/dynamic-config-store/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lostpebble/dynamic-config-store.git"
  },
  "license": "ISC",
  "author": "Paul Myburgh",
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "prepublish": "npm run clean && npm run build",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "immer": "^1.7.4",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.6",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6",
    "rollup": "^0.66.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.17.0"
  }
}
